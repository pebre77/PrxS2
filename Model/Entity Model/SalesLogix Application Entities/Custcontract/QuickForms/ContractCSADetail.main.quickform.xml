	Sage.Platform.ChangeManagement.PropertyChange prop9		= state.GetChangedState().FindPropertyChange("Nondisclosure");
	Sage.Platform.ChangeManagement.PropertyChange prop10	= state.GetChangedState().FindPropertyChange("Contractrelationship");
	Sage.Platform.ChangeManagement.PropertyChange prop11	= state.GetChangedState().FindPropertyChange("Contractbrand");
