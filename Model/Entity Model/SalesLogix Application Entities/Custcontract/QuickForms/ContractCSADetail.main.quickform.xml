	Sage.Platform.ChangeManagement.PropertyChange prop13	= state.GetChangedState().FindPropertyChange("Concomments");
	
	if (prop1 != null){
		sql="update OpportunityCIUDF set CSANUM='"+txtCSANum.Text+"' where Opportunityid='"+custc.Id.ToString()+"'";
		session.CreateQuery(sql)
		.ExecuteUpdate();	
	}
	
	if (prop2 != null){
		sql="update OpportunityCIUDF set ContractType='"+pklContractType.PickListValue+"' where Opportunityid='"+custc.Id.ToString()+"'";
		session.CreateQuery(sql)
		.ExecuteUpdate();	
	}
	
	if (prop3 != null){
		sql="update OpportunityCIUDF set Signaturedate='"+dtpSignatureDate.Text+"' where Opportunityid='"+custc.Id.ToString()+"'";
		session.CreateQuery(sql)
		.ExecuteUpdate();	
	}
	
	if (prop4 != null){
		sql="update OpportunityCIUDF set YearsOfService='"+txtYearsofservice.Text+"' where Opportunityid='"+custc.Id.ToString()+"'";
		session.CreateQuery(sql)
		.ExecuteUpdate();	
	}
	
	if (prop5 != null){
		sql="update OpportunityCIUDF set Assignable='"+txtAssignable.Text+"' where Opportunityid='"+custc.Id.ToString()+"'";
		session.CreateQuery(sql)
		.ExecuteUpdate();	
	}
	
	if (prop6 != null){
